SET AUTOCOMMIT OFF;

CREATE OR REPLACE TRIGGER TG_ESTADO
BEFORE INSERT ON ESTADO
FOR EACH ROW
BEGIN
    :NEW.ID := SEQ.NEXTVAL;
END;

